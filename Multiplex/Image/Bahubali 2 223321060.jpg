			{
				document.getElementById('div_most_commented').style.width = "1px";
				slideFromRight();
				glb_upanimation = 1;
			}
			document.getElementById('div_most_commented').style.top = (newtop - boxheight)  + "px";
		}
		else
		{
			document.getElementById('div_most_commented').style.top = (newtop - boxheight)  + "px";
		}
	}
}

function slideFromRight()
{
	var curwd = parseInt(document.getElementById('div_most_commented').style.width);
	if(curwd <= 480)
	{
		document.getElementById('div_most_commented').style.width = (curwd + 10)  + "px";
		window.setTimeout('slideFromRight()',10);
	}
}


